<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>

<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="org.progetto.client.GameView" prefHeight="400.0" prefWidth="600.0">


    <stylesheets>
        <URL value="@style/game_style.css" />
    </stylesheets>

    <GridPane fx:id="grid" layoutX="50.0" layoutY="50.0" gridLinesVisible="true">

    </GridPane>

    <Button layoutX="200.0" layoutY="239.0" onAction="#startGame" prefHeight="42.0" prefWidth="149.0" text="startGame" />
    <Button layoutX="200.0" layoutY="339.0" onAction="#pickComponent" prefHeight="42.0" prefWidth="149.0" text="pickComponent" />
    <Button layoutX="200.0" layoutY="439.0" onAction="#rotateComponent" prefHeight="42.0" prefWidth="149.0" text="rotate" />

    <VBox fx:id="components" alignment="CENTER" layoutX="210.0" layoutY="108.0"> </VBox>

</AnchorPane>
